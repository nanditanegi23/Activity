public class ConversionMApper {

	
public ConversionDTO convertProductToProductDTO(Conversion conversion) {
		
	ConversionDTO conversionDTO=new ConversionDTO();
	conversionDTO.setId(conversion.getConversionFactor());
	conversionDTO.setDiscount(conversion.getCountryCode());
		return conversionDTO;
		
	}
	
